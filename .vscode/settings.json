    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".vscode": true,
    "public": true,
    ".eslintrc.json": true,
    ".gitignore": true,
    "next-env.d.ts": true,
    "next.config.js": true,
